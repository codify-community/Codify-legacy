    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "@types/node": "^18.8.4",
    "discord.js": "^14.6.0",
    "dotenv": "^16.0.3",
    "tslog": "^3.3.4"
  }
