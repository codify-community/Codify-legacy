{
  "compilerOptions": {
    "target": "ES2021",
    "module": "CommonJS",
    "sourceRoot": "src",
    "rootDir": "src",
    "outDir": "out",
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "incremental": true,
    "inlineSourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "Node"
  }
}
