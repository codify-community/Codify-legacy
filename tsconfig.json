{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "strictNullChecks": true,
    "allowJs": false,
    "sourceMap": true,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "incremental": true,
    "outDir": "Target",
    "baseUrl": "./",
    "paths": {
      "@Commands/*": ["Sources/Commands/*"],
      "@Handlers/*": ["Sources/Handlers/*"]
    }
  }
}
