{
  "compilerOptions": {
    "target": "ESNext",
    "module": "CommonJS",
    "strict": true,
    "strictNullChecks": true,
    "allowJs": false,
    "esModuleInterop": true,
    "moduleResolution": "Node",
    "incremental": true,
    "outDir": "Target",
    "pretty": true,
    "noImplicitAny": true,
    "inlineSourceMap": true,
    "baseUrl": "./Sources",
    "paths": {
      "@codify/*": ["./*"],
      "@commands/*": ["Commands/*"]
    }
  }
}
